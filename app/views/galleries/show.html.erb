<div class="bannergallery">
      <div class= "title-gallery">
        <h1>Dive-in Ocean Gallery</h1>
      </div>
   </div>

<div class="container-gallery">
	<div class="gallery">

    <% @tags.each do |tag| %>
      <% if tag.photo.attached? %>
		    <div class="gallery-item">
          <%= cl_image_tag tag.photo.key, class: "gallery-image" %>
        </div>
      <% end %>
    <% end %>

	</div>

</div>
